                        .blog-detail__share
                            .share
                                .share__ttl Понравилась статья? Расскажите друзьям!
                                .share__ss
                                    include blocks/_share

            .subscribe
                .container
                    .subscribe__w2
                        .subscribe__ttl Лучшие статьи еженедельно в вашей почте

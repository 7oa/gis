extends core/_template

append sharedVars
    - var activeMenu = 'Блог';
    - var curPage = 'Создаем первую анимацию в ArcGIS Pro';

block title
    title=activeMenu

block content
    .workarea
        .page
            .blog-detail-top
            .blog-detail
                .container
                    .blog-detail__cnt
                        .blog-detail__head
                            .blog-detail__img(style="background-image: url('https://picsum.photos/2000/1000?image=654')")
                            .blog-detail__abs
                                .blog-detail__h
                                    span=curPage
                                .blog-detail__autor
                                    .blog-detail__autor-foto(style="background-image: url('https://picsum.photos/100/?image=1011');")
